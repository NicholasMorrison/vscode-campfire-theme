{
	"name": "Campfire",
	"type": "dark",
	"colors": {
		"editor.background": "#19191f",
		"editor.foreground": "#eeffff",
		"editorGroupHeader.tabsBackground": "#1f1f26",
		"activityBarBadge.background": "#930523",
		"sideBarTitle.foreground": "#bbbbbb",
		"statusBar.background": "#0e572e",
		"tab.inactiveBackground": "#1f1f26",
		"titleBar.activeBackground": "#19191f",
		"minimap.background": "#1f1f26",
		"editor.selectionBackground": "#d3963a2c",
		"sideBar.background": "#19191f",
		"scrollbarSlider.hoverBackground": "#3f3e3e",
		"menu.background": "#19191f",
		"activityBar.background": "#19191f",
		"activityBar.activeBackground": "#19191f",
		"sideBarSectionHeader.background": "#1f1f26",
		"quickInput.background": "#19191f",
		"quickInputList.focusBackground": "#bd5f6d",
		"editorOverviewRuler.findMatchForeground": "#f1f1f1",
		"list.activeSelectionBackground": "#f14862",
		"list.focusOutline": "#f14862"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#454643"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"variable.other.readwrite.alias",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#afafaf"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#ff0000",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#f14862"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#f14862"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter",
				"support.class.component"
			],
			"settings": {
				"foreground": "#598d60"
			}
		},
		{
			"name": "Tag Brackets / Text Children",
			"scope": [
				"punctuation.definition.tag.begin",
				"punctuation.definition.tag.end",
				"meta.jsx.children",
				"punctuation.section.embedded.begin",
				"punctuation.section.embedded.end",
				"meta.brace.round",
				"meta.brace.square"
			],
			"settings": {
				"foreground": "#f1f1f1"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"meta.definition.variable",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#f87760"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"source.python",
				"punctuation",
				"meta.block.variable.other",
				"meta.object-literal.key"
			],
			"settings": {
				"foreground": "#afafaf"
			}
		},
		{
			"name": "Object Attribute",
			"scope": [
				"variable.other.property",
				"variable.other.object.property"
			],
			"settings": {
				"foreground": "#efefef"
			}
		},
		{
			"name": "Other Variable, Function Argument, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link",
				"variable.parameter",
				"variable.other.readwrite"
			],
			"settings": {
				"foreground": "#afafaf"
			}
		},
		{
			"name": "Constant, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#930523"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#d3830b"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end",
				"punctuation.definition.string.template.begin.js",
				"punctuation.definition.string.template.end.js",
				"punctuation.definition.template-expression.begin.js",
				"punctuation.definition.template-expression.end.js",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#79c091"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
